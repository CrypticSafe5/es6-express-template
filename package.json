{
  "name": "es6-template",
  "version": "1.0.1",
  "description": "Template for es6 express application",
  "main": "./dist/app.js",
  "scripts": {
    "test": "jest",
    "clean": "rimraf ./dev & rimraf ./dist & rimraf ./node_modules & rimraf package-lock.json && npm i",
    "dev": "rimraf ./dev && babel --preset @babel/preset-env -d ./dev ./src && node dev/app",
    "build": "rimraf ./dist && babel --minified --compact true --presets @babel/preset-env -d ./dist ./src && node dist/app"
  },
  "author": "CrypticSafe5",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "rimraf": "^3.0.0"
  }
}
